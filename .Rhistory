library(tmap)
library(leaflet)
dexameni <- read_sf("../dexameni_poly.shp")
farmakeio <- read_sf("../farmakeio.shp")
mandra <- read_sf("../mandra.shp")
tmap_options(check.and.fix = T)
tmap_mode("view")
tm_basemap(leaflet::providers$Stamen.Watercolor) + tm_shape(dexameni) + tm_polygons()
tm_shape(dexameni) + tm_polygons()
dexameni <- read_sf("../dexameni_poly.shp")
farmakeio <- read_sf("../farmakeio.shp")
mandra <- read_sf("../mandra.shp")
# Use tmap options to set the basemap and overlay map permanently during the R session:
opts <- tmap_options(basemaps = c(Canvas = "Esri.WorldGrayCanvas", Imagery = "Esri.WorldImagery"),
overlays = c(Labels = paste0("http://services.arcgisonline.com/arcgis/rest/services/Canvas/",
"World_Light_Gray_Reference/MapServer/tile/{z}/{y}/{x}")))
tmap_options(check.and.fix = T)
tmap_mode("view")
# restore options
tmap_options(opts)
tm_shape(dexameni) + tm_polygons()
dexameni <- read_sf("../dexameni_poly.shp")
farmakeio <- read_sf("../farmakeio.shp")
mandra <- read_sf("../mandra.shp")
# Use tmap options to set the basemap and overlay map permanently during the R session:
opts <- tmap_options(basemaps = c(Canvas = "OpenStreetMap", Imagery = "OpenStreetMap"),
overlays = c(Labels = paste0("https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png")))
tmap_options(check.and.fix = T)
tmap_mode("view")
# restore options
tmap_options(opts)
tm_shape(dexameni) + tm_polygons()
